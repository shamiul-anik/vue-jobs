{
  "name": "vue-jobs",
  "version": "1.0.0",
  "description": "Full-stack Job Board Application - Monorepo Structure",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "setup": "npm install && cd frontend && npm install && cd ../backend && npm install && cd ..",
    "dev": "concurrently \"cd backend && npm run dev\" \"cd frontend && npm run dev\"",
    "build": "cd frontend && npm run build",
    "preview": "cd frontend && npm run preview",
    "server": "cd backend && npm start",
    "frontend:dev": "cd frontend && npm run dev",
    "backend:dev": "cd backend && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "db:benchmark": "cd backend && npm run db:benchmark"
  },
  "keywords": [
    "vue",
    "jobs",
    "board",
    "express",
    "sqlite"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^9.2.1"
  }
}
